{
  "name": "@appbuild/ui-foundation",
  "version": "0.1.0",
  "description": "Polished, native-feeling UI components for React Native + Web",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tokens": {
      "import": "./dist/tokens/index.mjs",
      "require": "./dist/tokens/index.js"
    },
    "./themes": {
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint src --ext .js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamhollisterbot/app-build-ui-foundation.git"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "design-system",
    "theming"
  ],
  "author": "Adam",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.73.0",
    "react-native-reanimated": ">=3.0.0"
  },
  "dependencies": {
    "expo-haptics": "^14.0.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.15",
    "@storybook/react-webpack5": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.2",
    "react-native-reanimated": "3.16.0",
    "react-native-web": "^0.21.2",
    "storybook": "^8.6.15",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
